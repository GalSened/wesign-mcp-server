# WeSign MCP Server Configuration
# Copy this file to .env and update with your credentials

# WeSign API URL (use production or dev/test)
WESIGN_API_URL=https://devtest.comda.co.il

# WeSign Account Credentials
WESIGN_EMAIL=your-email@example.com
WESIGN_PASSWORD=your-password

# Session persistence (true/false)
WESIGN_PERSISTENT=false

# Server Port (default: 8080)
PORT=8080
